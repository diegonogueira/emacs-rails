module Admin::ReportsHelper
 def generateurl(profile)
 	return url_for :controller => 'profiles', :id => profile, :only_path => false
 end

 def generatelink(report,partner)
    link1 = link_to "PDF", :controller=> "reports", :action => "#{report.to_s}", :partner => "#{partner.to_s}", :filetype => "pdf"
    link2 = link_to "CSV", :controller=> "reports", :action => "#{report.to_s}", :partner => "#{partner.to_s}", :filetype => "csv"
    return "#{link1} | #{link2}"
 end
end
